FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/fuel";
    object      pinId;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    zmin
    {
        type            calculated;
        value           uniform 16000;
    }
    zmax
    {
        type            calculated;
        value           uniform 16000;
    }
    fuel_to_cladding
    {
        type            calculated;
        value           uniform 16000;
    }
}


